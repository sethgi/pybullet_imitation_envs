<?xml version="1.0" ?>
<robot name="t_object">

  <link name="base">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>

    <!-- Stem (vertical part of T, but flat on XY plane) -->
    <visual>
      <origin xyz="0 -0.05 0.025" rpy="0 0 0"/>
      <geometry>
        <box size="0.05 0.10 0.05"/>
      </geometry>
      <material name="blue">
        <color rgba="0.1 0.1 0.8 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 -0.05 0.025" rpy="0 0 0"/>
      <geometry>
        <box size="0.05 0.10 0.05"/>
      </geometry>
    </collision>

    <!-- Crossbar (horizontal part of T) -->
    <visual>
      <origin xyz="0 0.025 0.025" rpy="0 0 0"/>
      <geometry>
        <box size="0.15 0.05 0.05"/>
      </geometry>
      <material name="green">
        <color rgba="0.1 0.8 0.1 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0.025 0.025" rpy="0 0 0"/>
      <geometry>
        <box size="0.15 0.05 0.05"/>
      </geometry>
    </collision>

  </link>

</robot>
